            "value": "sincurve"
        },
        "7": {
            "type": "set",
            "target": "method_protocol",
            "value_type": "protocol",
            "value": "none"
        },
        "8": {
            "type": "blank"
        },
        "9": {
            "type": "comment",
            "content": " 放置头部信息"
        },
