            "value": {
                "method": "cross",
                "method_dur": 10
            }
        },
        "4": {
            "type": "set",
            "target": "bb_method_default",
            "value_type": "method",
            "value": {
                "method": "cross",
                "method_dur": 10
            }
        },
        "5": {
            "type": "set",
            "target": "bg_method_default",
